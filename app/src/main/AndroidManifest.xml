<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zozancan.retrofitchallenge">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".network.RetrofitChallengeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".photos.PhotosDetailActivity"></activity>
        <activity android:name=".photos.PhotosActivity" />
        <activity android:name=".album.AlbumActivity" />
        <activity android:name=".postlist.PostListActivity" />
        <activity android:name=".userlist.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>